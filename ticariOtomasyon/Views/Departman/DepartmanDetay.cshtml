@using ticariOtomasyon.Models.Siniflar
@model List<Personel>
@{
    ViewBag.Title = "DepartmanDetay";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />

<h2>Departman Detay Sayfası</h2>
<br />
<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active">Departman Ad >@ViewBag.s</li>
</ol>

<table class="table table-hover">
    <tr>
        <td>Personel</td>
        <td>Satışlar</td>

    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.PersonelAd @s.PersonelSoyad</td>
            <td><a href="/Departman/DepartmanPersonelSatis/@s.Personelid" class="btn btn-default" style="background-color:darksalmon">Satışlar</a></td>
        </tr>
    }
</table>